   "source": [
    "l = [10, 5, 2, 6, 1, 3]"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 2,
   "outputs": [],
   "source": [
    "def bubble_sort(li):\n",
    "    for i in range(len(li) - 1):\n",
    "        for j in range(len(li) - 1 - i):\n",
    "            if li[j] > li[j + 1]:\n",
    "                li[j], li[j + 1] = li[j + 1], li[j]"
   ],
   "metadata": {
    "collapsed": false
   }
  },
  {
   "cell_type": "code",
   "execution_count": 3,
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "[10, 5, 2, 6, 1, 3]\n",
      "[1, 2, 3, 5, 6, 10]\n"
     ]
    }
   ],
   "source": [
    "print(l)\n",
    "bubble_sort(l)\n",
    "print(l)"
   ],
